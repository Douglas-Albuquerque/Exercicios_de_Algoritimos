algoritmo "EXERCICIO_42_contador_N1"
var
   contador, N1 :inteiro
inicio
      Escreva("Digite um numero inteiro: ")
      leia(N1)
      contador <- 0
      Enquanto (contador <= N1) faca
               Escreval(contador)
               contador <- contador+1
      fimEnquanto
      Escreva("Acabou!")

fimalgoritmo